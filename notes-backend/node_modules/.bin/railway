#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/cli/bin/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/cli/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/cli/bin/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/cli/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules/@railway/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/@railway+cli@3.3.1/node_modules:/home/joselugz03/Escritorio/Dev/FullStack-Open/notes-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@railway/cli/bin/railway.js" "$@"
else
  exec node  "$basedir/../@railway/cli/bin/railway.js" "$@"
fi
